<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lifelonglearningapp">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".LifelongApp"
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LifelongLearningApp"
        >

        <meta-data
            android:name="com.google.android.geo.API.KEY"
            android:value="AIzaSyARb2vSaOD4Y49muMWzlQ4HVt3VKdQWVag"

            />
        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="sux2xu5d7l" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".LectureInformationActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".SearchActivity"
            android:exported="false"
            android:theme="@style/afterSearch" />

    </application>

</manifest>